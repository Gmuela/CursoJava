<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ejemplogoogleapi.cursomaana.example.com.ejemplostorage">

    <application
        android:allowBackup="true"
        android:icon="@drawable/android_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ListaUsuariosActivity" />
        <activity android:name=".activities.DetalleUsuarioActivity" />
        <activity android:name=".activities.ActualizarUsuarioActivity" />
        <activity android:name=".activities.CrearUsuarioActivity"/>
    </application>

</manifest>